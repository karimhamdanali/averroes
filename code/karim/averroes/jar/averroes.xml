<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="Create Runnable Jar for Project averroes with Jar-in-Jar Loader">
    <!--this file was created by Eclipse Runnable JAR Export Wizard-->
    <!--ANT 1.7 is required                                        -->
    <target name="create_run_jar">
        <jar destfile="averroes.jar">
            <manifest>
                <attribute name="Main-Class" value="org.eclipse.jdt.internal.jarinjarloader.JarRsrcLoader"/>
                <attribute name="Rsrc-Main-Class" value="ca.uwaterloo.averroes.jar.JarFactory"/>
                <attribute name="Class-Path" value="."/>
                <attribute name="Rsrc-Class-Path" value="./ bcel-5.2.jar sootclasses-2.5.0.jar probe.jar jasminclasses-2.5.0.jar polyglotclasses-1.3.5.jar"/>
            </manifest>
            <zipfileset src="jar-in-jar-loader.zip"/>
            <fileset dir="../bin"/>
            <zipfileset dir="../lib" includes="bcel-5.2.jar"/>
            <zipfileset dir="../lib" includes="sootclasses-2.5.0.jar"/>
            <zipfileset dir="../lib" includes="probe.jar"/>
            <zipfileset dir="../lib" includes="jasminclasses-2.5.0.jar"/>
            <zipfileset dir="../lib" includes="polyglotclasses-1.3.5.jar"/>
        </jar>
    </target>
</project>
